{
	"name": "detain/myadmin-cpanel-webhosting",
	"type": "myadmin-plugin",
	"description": "Cpanel Webhosting Class",
	"keywords": [
		"cpanel",
		"administration",
		"webhosting"
	],
	"license": "LGPL-2.1-only",
	"authors": [
		{
			"name": "Joe Huss",
			"homepage": "https:\/\/my.interserver.net\/"
		}
	],
	"config": {
		"bin-dir": "vendor\/bin",
		"minimum-stability": "dev"
	},
	"require": {
		"php": ">=5.0.0",
		"ext-soap": "*",
		"symfony/event-dispatcher": ">=2.8.26",
		"cpanelinc/xmlapi-php": ">=2.0.1",
		"detain/myadmin-plugin-installer": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
		"phpunit/phpunit-mock-objects": "*",
		"vlucas/phpdotenv": "*",
		"codeclimate/php-test-reporter": "dev-master",
		"satooshi/php-coveralls": "*",
		"codacy/coverage": "dev-master"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "cpanelinc/xmlapi-php",
				"version": "2.0.1",
				"source": {
					"url": "https://github.com/CpanelInc/xmlapi-php.git",
					"type": "git",
					"reference": "master"
				}
			}
		}
	],
	"autoload": {
		"psr-4": {
			"Detain\\MyAdminCpanel\\": "src/"
		}
	}
}
